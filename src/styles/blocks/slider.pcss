.slider__header{
    display: flex;
    margin-bottom: 10%;
}
.slider__title{
    font-size: 36px;
    font-weight: 700;
    margin-right: 115px;
}
.slider__content{
    display: flex;
    justify-content: space-between;
    padding: 0 8%;
}
.slider__item{
    margin-right: 10%;
    &:last-child{
        margin-right: 0;
    }
}