.iconed-link{
    display: flex;
    color: $links-color;
    font-size: 18px;
    font-weight: 700;
    &:hover{
        color: $darkBlue;
        opacity: 1;
    }
    @include tablets{
        width: 100%;
        justify-content: center;
    }
    @include phones{
        justify-content: start;
    }
}
.iconed-link__icon{
    margin-right: 5%;
    width: 20px;
    fill: $links-color;
}